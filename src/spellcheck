#!/bin/bash
spell -n story.zil -b | grep -v -Ev "DESC|LIGHTBIT|CRLF|HLIGHT|FCLEAR|COND|LTABLE|PRECHOICE|FSET|PUTP|WORNBIT|WEARBIT|SETG|AUX|GETP|ALLMONEY|RTRUE|RFALSE|EMPHASIZE|READLINE|LEXBUF|GETB|CONTBIT|OPENBIT|NARTICLEBIT|PLURALBIT|PERSONBIT|TOUCHBIT|PRINTN|GIVEBAG|SATORI|LEVAD|VENEFIX|: CT$|OBJ$|: suppl$|: ies$" | grep -i -v -Ev "cesspit|rulership|dozily|guildmaster|gasbag|dustbowl|folklorists|snuck" | grep -i -v -Ev ": gleenars$|: judain$|: godorno$|: sycaari$|: talmai$|: talmai's$|: marmeluke$|: ginath$|: ginath's$|: skakshi$|: melmelo$|: melmelo's$|: ichor$|: tormil$|: lucie$|: lucie's$|: fortuny$|: megiddo$|: grond$|: acennon$|: del$|: raff$|: harakadnezzar$|: kush$|: panopticon$|: tarkamandir$|: manta$|: greenbark$" |uniq
